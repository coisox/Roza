[
	{
		"type" : "standardlist",
		"labelbi" : "Task List",
		"labelbm" : "Senarai Tugasan",
		"onclick" : "rozaCallLandingFile('UI_ProtoCuti.js?id={{ROZA_ID}}')",
		"source" : "SELECT task_id ROZA_ID, task_title ROZA_TITLE, task_desc ROZA_DESC, task_timecreate ROZA_TIME, task_unread ROZA_UNREAD, '{{globalUserName}}' CUBAAN1, ? CUBAAN2 FROM proto_task WHERE task_userid = ? ORDER BY task_timecreate",
		"sourceparam" : [
			"Selangor",
			"{{globalUserId}}"
		]
	}
]

